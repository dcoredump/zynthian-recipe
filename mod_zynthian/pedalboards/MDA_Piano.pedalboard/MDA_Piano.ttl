    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/vel_to_hardness>
    ingen:value 50.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/env_decay>
    ingen:value 50.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/fine_tuning>
