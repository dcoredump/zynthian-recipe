        lv2:InputPort .

<Piano/vel_to_muffling>
    ingen:value 25.100000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/muffling_filter>
    ingen:value 80.300003 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/vel_sensitivity>
    ingen:value 37.599998 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/polyphony>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Piano/stereo_width>
